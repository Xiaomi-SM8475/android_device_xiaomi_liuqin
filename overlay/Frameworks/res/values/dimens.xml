    <!-- Height of the status bar in portrait. The height should be
         Max((status bar content height + waterfall top size), top cutout size) -->
    <dimen name="status_bar_height_portrait">60.0px</dimen>
    <dimen name="status_bar_height_landscape">60.0px</dimen>

    <!-- Height of a small notification in the status bar -->
    <dimen name="notification_min_height">106.0dip</dimen>

    <!-- Rounded corner settings for secondary built-in display -->
    <dimen name="rounded_corner_radius">46.0px</dimen>
    <dimen name="rounded_corner_radius_bottom">46.0px</dimen>
    <dimen name="rounded_corner_radius_top">46.0px</dimen>

    <!-- Height of area above QQS where battery/time go (equal to status bar) -->
    <dimen name="quick_qs_offset_height">60.0px</dimen>
</resources>