<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Whether to intercept inputs on screen off -->
    <bool name="config_enableKeyHandler">true</bool>

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">143</integer>

    <!-- Default settings for gestures:
             0 - Disabled
             1 - Wake
             2 - Pulse
             3 - Flashlight
             4 - Airplane
             5 - Play/Pause
             6 - Next
             7 - Previous
             8 - Camera
             9 - Dialer -->
    <integer name="config_doubleTapDefault">1</integer>

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <string name="config_oemFastChargerStatusPath" translatable="false">/sys/class/qcom-battery/fastchg_mode</string>

    <!-- Fingerprint from stock ROM -->
    <string name="config_stockFingerprint" translatable="false">Xiaomi/liuqin/missi:13/TKQ1.221114.001/V14.0.6.0.TMYCNXM:user/release-keys</string>

    <!-- Device model (Build.MODEL) to spoof for netflix -->
    <string name="config_netflixSpoofModel" translatable="false">23043RP34G</string>

    <!-- ComponentName of the default dream (Settings.Secure.DEFAULT_SCREENSAVER_COMPONENT) -->
    <string name="config_dreamsDefaultComponent">com.google.assistant.hubui/com.google.assistant.hubui.dream.CuratedPhotoDreamService</string>

    <!-- ComponentNames of the dreams that we should hide -->
    <string-array name="config_disabledDreamComponents">
        <item>com.google.android.deskclock/com.android.deskclock.Screensaver</item>
        <item>com.android.dreams.basic/.Colors</item>
        <item>com.google.android.apps.photos/.daydream.PhotosDreamService</item>
        <item>com.android.systemui/com.google.android.systemui.lowlightclock.LowLightClockDreamService</item>
    </string-array>
</resources>
